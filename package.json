{
  "name": "whatsapp-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./bin/www",
    "production": "yarn link rs-user && NODE_ENV=production node ./bin/www",
    "client": "cross-env NODE_ENV=development cd ./client && yarn start",
    "all": "cross-env NODE_ENV=development npm-run-all -p start client",
    "build": "cross-env NODE_ENV=production yarn install && cd ./client && yarn install && yarn run build"
  },
  "repository": "https://github.com/Dipeshlohani/whatsapp-bot.git",
  "author": "Bibek Gaihre <bibekgaihre123@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "actions-on-google": "^2.12.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "config": "^3.2.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "dialogflow-fulfillment": "0.6.1",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "moment": "^2.24.0",
    "mongoose": "^5.7.13",
    "morgan": "^1.9.1",
    "twilio": "^3.38.0"
  }
}
